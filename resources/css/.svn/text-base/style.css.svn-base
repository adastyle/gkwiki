}

#news_list>.video_item .div_img {
	padding: 0rem 1rem;
}

.live_item {
	width: 100%;
	background-color: #fff;
}

.live_item .div_img {
	position: relative;
}

.live_item .cover {
	width: 100%;
}

.live_item .title {
	position: absolute;
	top: 10px;
	left: 18px;
	right: 18px;
	color: #fff;
	font-size: 1.0625rem;
	line-height: 1.6rem;
}

.live_item .div_avatar {
	position: absolute;
	bottom: -6px;
	left: 14px;
	border: 1px solid #fff;
	border-radius: 19px;
	width: 38px;
	height: 38px;
}

.live_item .div_avatar>.avatar {
	width: 2.25rem;
	height: 2.25rem;
	border-radius: 1.125rem;
	background-color: #fff;
}

.live_item .time_div {
	position: absolute;
	top: 42%;
	width: 100%;
	text-align: center;
}

.live_item .time_div>div {
	padding: 2px 12px;
	background-color: hsla(0, 0%, 0%, 0.6);
	border-radius: 9.375rem;
	margin: 0 auto;
	display: inline-block;
	text-align: center;
}

.live_item .time_div .icon {
	line-height: 1.4rem;
	font-size: 0.875rem;
	color: #5D85DE;
	margin-top: 1px;
	margin-right: 0.25rem;
}

.live_item .time_div .icon.live {
	color: #FF6363;
}

.live_item .time_div .time {
	font-size: 0.875rem;
	color: #fff;
	line-height: 1.4rem;
}
/*评论输入框样式*/
.comment_div {
	position: absolute;
	z-index: 10000;
	bottom: 0;
	left: 0;
	width: 100%;
	background-color: #F5F5F5;
	height: 48px;
	padding: 6px 12px;
	border-top: 0.5px solid #CCCCCC;
}

.comment_div .comment_input {
	height: 34px;
	border: 0.5px solid #DCDCDC;
	background-color: #fff;
	border-radius: 18px;
	padding: 4px 10px;
	right: 76px;
	position: absolute;
	left: 16px;
}

.comment_div .comment_input i {
	font-size: 30px;
	line-height: 26px;
	color: #999999;
}

.comment_div .comment_right {
	float: right;
	font-size: 15px;
	line-height: 32px;
	color: #666666;
}

.comment_div input {
	font-size: 14px;
	height: 26px;
	width: 85%;
	border: 0;
	padding: 0 4px;
	vertical-align: top;
	margin-bottom: 0;
}

