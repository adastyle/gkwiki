}

.detail_head_line {
	height: 1px;
	width: 100%;
	background-color: #e6e6e6;
}

.detail_head_title {
	position: absolute;
	top: -10px;
	z-index: 10;
	width: 100%;
	text-align: center;
	font-size: 13px;
	color: #282628;
	font-weight: bold;
}

.detail_head_title>div {
	display: inline-block;
	padding: 0 0.6rem;
	background-color: #fff;
}

.comment_item {
	position: relative;
	padding: 0.5rem 0;
}

.comment_item .comment_avatar {
	width: 2rem;
	height: 2rem;
	border-radius: 50%;
	margin-left: 0.5rem;
}

.comment_item>div>div {
	width: calc(100% - 60px);
