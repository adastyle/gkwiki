/*调试状态样式inherit，非调试状态时display:none*/

.debug {
	display: none;
}

/*通用元素样式*/

html {
	font-size: 16px !important;
	/*overflow-x: hidden;*/
}

body {
	background-color: #F4F5F6;
	margin: 0;
	padding: 0;
	font-size: 1rem;
	display: inline-block;
	overflow-x: hidden;
	width: 100%;
}

p {
	font-size: inherit;
	color: inherit;
}

pre {
	display: inline-block;
	margin: 0;
}

div {
	vertical-align: top;
}

a {
	text-decoration: none;
}

body>.mui-progressbar {
	top: 2.8rem;
}

ul {
	padding: 0;
	margin: 0;
}

li {
	list-style-position: inside;
	font-size: 1rem;
	list-style-type: none;
}

.mui-content {
	/*height: 100%;*/
	bottom: 0;
	background-color: #fff;
}

.mui-input-group {
	margin-top: 10px;
}

.mui-input-group:first-child {
	margin-top: 20px;
}

a.mui-action-back {
	color: #231815;
}

.mui-bar-nav~.mui-content {
	margin-top: 2.8rem;
	padding-top: 0;
}

.mui-bar~.mui-scroll-wrapper {
	margin-top: 2.8rem;
}


/*通用样式定义*/

.inline {
	display: inline-block;
}

.absolute {
	position: absolute;
	left: 0;
	top: 0;
}

.more {
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}

.more2 {
	overflow: hidden;
	display: -webkit-box !important;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	word-break: break-all;
	white-space: normal !important;
}

.more3 {
	overflow: hidden;
	display: -webkit-box !important;
	-webkit-line-clamp: 3;
	-webkit-box-orient: vertical;
	word-break: break-all;
	white-space: normal !important;
}

.more4 {
	overflow: hidden;
	display: -webkit-box !important;
	-webkit-line-clamp: 4;
	-webkit-box-orient: vertical;
	word-break: break-all;
	white-space: normal !important;
}

.more5 {
	overflow: hidden;
	display: -webkit-box !important;
	-webkit-line-clamp: 5;
	-webkit-box-orient: vertical;
	word-break: break-all;
	white-space: normal !important;
}

.more6 {
	overflow: hidden;
	display: -webkit-box !important;
	-webkit-line-clamp: 6;
	-webkit-box-orient: vertical;
	word-break: break-all;
	white-space: normal !important;
}

.m-l-4 {
	margin-left: 0.25rem;
}

.m-t-10 {
	margin-top: 0.625rem;
}

/* checkbox checkbox样式*/
.mui-checkbox, .mui-radio {
	display: inline-block;
}

.mui-checkbox.mui-left input[type=checkbox],
.mui-radio.mui-left input[type=radio] {
	left: 0;
	width: auto;
	height: auto;
}

.mui-checkbox.mui-left label,
.mui-radio.mui-left label {
	padding-right: 0.375rem;
	padding-left: 2.125rem;
}

.mui-checkbox input[type=checkbox]:checked:before, .mui-radio input[type=radio]:checked:before {
	color: #008FD3;
}

.mui-checkbox input[type=checkbox], .mui-radio input[type=radio] {
	top: 0.125rem;
}

/*titlebar相关样式*/

.mui-title {
	color: #282628;
	font-size: 1.11765rem;
	height: 2.8rem;
	line-height: 2.8rem;
	font-family: "苹方";
}

.mui-bar-nav {
	background-color: #fff;
	color: #231815;
}

.bar_logo {
	width: 7rem;
	margin: 0.375rem;
}

.bar_input {
	position: absolute;
	left: 7.75rem;
	bottom: 0.5rem;
	right: 3.375rem;
	height: 1.75rem;
}

.bar_input>input {
	border: #026A7B;
	font-size: 0.875rem;
	height: 1.75rem;
}

.bar_message_btn {
	position: absolute;
	bottom: 0.5rem;
	right: 0.5rem;
	height: 1.875rem;
	width: 2.25rem;
	text-align: center;
}

.bar_message_btn i {
	font-size: 1.375rem;
	line-height: 1.875rem;
	color: #fff;
}

/*搜索按钮样式*/
.search {
	position: absolute;
	background-color: transparent !important;
    right: 0;
    bottom: 0.45rem;
	border: none;
	color: #4A90E2;
}

/*jPlayer样式*/
.jp-container {
	position: relative;
}

.jp-controll-div {
	position: absolute;
	width: 100%;
	top: 0;
	bottom: 0;
	background-color: rgba(0, 0, 0, 0.4392156862745098);
	text-align: center;
	color: #FCFAFB;
}

.jp-controll-div .jp-progress .jp-seek-bar {
	width: 100%;
	height: 2px;
	background-color: #70635A;
}

.jp-controll-div .jp-progress .jp-seek-bar .jp-play-bar {
	height: 100%;
	background-color: #B05854;
	position: relative;
	overflow: visible !important;
	/*background-color: #E8E2D6;*/
}

.jp-play-bar-dot {
	width: 16px;
	height: 16px;
	border-radius: 8px;
	background-color: #FCFAFB;
	position: absolute;
	right: -10px;
	top: -7px;
}

.jp-controll-div .jp-bottom-bar {
	width: 100%;
	bottom: 16px;
	position: absolute;
}

.jp-controll-div .jp-progress {
	position: absolute;
	left: 62px;
	right: 106px;
	top: 9px;
}

.jp-play {
	display: inline-block;
	margin: 0 auto;
	margin-top: 24%;
}

.jp-current-time {
	display: inline-block;
	float: left;
	margin-left: 16px;
	font-size: 0.875rem;
}

.jp-bottom-bar-right {
	float: right;
	margin-right: 16px;
}

.jp-duration {
	margin-right: 20px;
	display: inline-block;
	font-size: 0.875rem;
}

.jp-full-screen {
	display: inline-block;
}

.jp-full-screen>i {
	font-size: 1.2rem;
}

.jp-play:before {
	color: #FCFAFB;
	font-size: 2rem;
	content: "\e655";
}

.jp-state-playing .jp-play:before {
	content: "\e605";
}

/*tabbar相关样式*/

.mui-bar-tab {
	background-color: #fff;
	height: 3rem !important;
}

.mui-bar {
	-webkit-box-shadow: 0 0 1px #d6d6d6;
	box-shadow: 0 0 1px #d6d6d6);
	height: auto;
	padding: 0;
}

.mui-bar-tab .mui-tab-item {
	height: auto;
}

.mui-bar-tab .mui-tab-item.mui-active {
	color: #4A90E2!important;
}

.mui-bar-tab .mui-tab-item .iconfont~.mui-tab-label {
	font-size: 0.625rem;
	display: block;
	overflow: hidden;
	text-overflow: ellipsis;
	margin-top: 0.1rem;
	margin-bottom: -0.1rem;
}

.mui-bar-tab .mui-tab-item .iconfont {
	top: 0.35rem;
	font-size: 1.5rem;
	padding-top: 0;
	padding-bottom: 0;
}

.mui-action-back {
	width: 2.8rem;
	text-align: center;
}

.mui-action-back .iconfont {
	line-height: 2.8rem;
	height: 2.8rem;
	font-size: 1.5rem;
}

.mui-bar .iconfont {
	position: relative;
	z-index: 20;
}

.iconfont .mui-badge {
	font-size: 10px;
	line-height: 1.4;
	position: absolute;
	top: -2px;
	left: 100%;
	margin-left: -10px;
	padding: 1px 5px;
	color: #fff;
	background: red;
}


/*下拉刷新、上拉加载列表样式*/

.mui-bar~.mui-content .mui-fullscreen {
	top: 2.8rem !important;
	height: auto;
}

.mui-pull-top-tips {
	position: absolute;
	top: -20px;
	left: 50%;
	margin-left: -25px;
	width: 40px;
	height: 40px;
	border-radius: 100%;
	z-index: 1;
}

.mui-bar~.mui-pull-top-tips {
	top: 24px;
}

.mui-pull-top-wrapper {
	width: 42px;
	height: 42px;
	display: block;
	text-align: center;
	background-color: #efeff4;
	border: 1px solid #ddd;
	border-radius: 25px;
	background-clip: padding-box;
	box-shadow: 0 4px 10px #bbb;
	overflow: hidden;
}

.mui-pull-top-tips.mui-transitioning {
	-webkit-transition-duration: 200ms;
	transition-duration: 200ms;
}

.mui-pull-top-tips .mui-pull-loading {
	/*-webkit-backface-visibility: hidden;
					-webkit-transition-duration: 400ms;
					transition-duration: 400ms;*/
	margin: 0;
}

.mui-pull-top-wrapper .mui-icon,
.mui-pull-top-wrapper .mui-spinner {
	margin-top: 7px;
}

.mui-pull-top-wrapper .mui-icon.mui-reverse {
	/*-webkit-transform: rotate(180deg) translateZ(0);*/
}

.mui-pull-bottom-tips {
	text-align: center;
	background-color: #efeff4;
	font-size: 15px;
	line-height: 40px;
	color: #777;
}

.mui-pull-top-canvas {
	overflow: hidden;
	background-color: #fafafa;
	border-radius: 40px;
	box-shadow: 0 4px 10px #bbb;
	width: 40px;
	height: 40px;
	margin: 0 auto;
}

.mui-pull-top-canvas canvas {
	width: 40px;
}

.mui-pull-bottom-wrapper {
	background: #fff;
}

.mui-slider .mui-slider-group .mui-slider-item .logo_img {
	width: 30%;
}

/*下拉刷新、上拉加载列表样式结束*/

/*支付组件样式开始*/
.pay_menu {
	display: none;
	background-color: hsla(0, 0%, 0%, 0.5);
}

.pay_menu .pay_bar {
	position: absolute;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 10000;
	background-color: #fff;
	padding: 0 1.5rem;
	height: 8rem;
}

.pay_menu .pay_bar .pay_btn {
	display: inline-block;
	width: 49%;
	text-align: center;
	height: 100%;
	padding-top: 2.5rem;
}

.pay_menu .pay_bar .pay_btn i {
	font-size: 2.6rem;
}

.pay_menu .pay_bar .pay_btn div {
	margin-top: 0.5rem;
	font-size: 1rem;
}

.pay_menu .pay_bar .pay_btn.ali_pay i {
	color: #0095e6;
}

.pay_menu .pay_bar .pay_btn.wx_pay i {
	color: #48bb2c;
}
/*支付组件样式结束*/


/*新闻页分类横向滚动条相关样式*/

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item.mui-active {
	color: #4A90E2;
}

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item.mui-active {
	border-bottom: 2px solid #4A90E2;
}

.mui-segmented-control {
	font-size: 0.9rem;
	background-color: #fff;
}

.mui-slider .mui-segmented-control.mui-segmented-control-inverted~.mui-slider-group .mui-slider-item {
	border: 0;
}

.mui-fullscreen .mui-segmented-control~.mui-slider-group {
	top: 2.2rem;
}

.mui-segmented-control.mui-scroll-wrapper {
	height: 2.1rem;
}

.mui-segmented-control.mui-scroll-wrapper .mui-scroll {
	height: 2.1rem;
}

.mui-segmented-control .mui-control-item {
	font-size: 1rem;
	line-height: 2.1rem;
}

.mui-segmented-control.mui-scroll-wrapper .mui-control-item {
	padding: 0 0.375rem 0 0.875rem;
}

/*轮播区域样式*/

.mui-fullscreen.mui-slider .mui-slider-item>a {
	top: 0;
	-webkit-transform: none;
	transform: none;
}

.mui-slider-item>a>.img_banner {
	width: 100%;
	/*height: 11rem;*/
}

.mui-slider-item>a>div {
	position: absolute;
	bottom: 0rem;
	left: 0;
	width: 100%;
	background-color: #000;
	opacity: 0.8;
	height: 30px;
}

.mui-slider-title {
	line-height: 30px;
	height: 30px;
	margin: 0;
	color: #fff;
	text-align: left;
	text-indent: 12px;
	width: 80%;
	display: block;
	overflow: hidden;
	/*单行*/
	white-space: nowrap;
	-o-text-overflow: ellipsis;
	text-overflow: ellipsis;
}

.slider-indicator {
	position: absolute;
	bottom: 0;
	width: 100%;
	text-align: center;
	background: rgba(0, 0, 0, 0.4) 0 0;
	height: 40px;
	line-height: 40px;
}

.slider-indicator>div:first-child {
	text-align: left;
	width: 80%;
	float: left;
	color: #ffffff;
	font-size: 13px;
	margin-left: 6px;
}

.mui-indicator.mui-active {
	background: #1790d0!important;
}

.mui-slider-indicator .mui-indicator {
	display: inline-block;
	width: 6px;
	height: 6px;
	margin: -1px 6px;
	cursor: pointer;
	border-radius: 0;
	background: #aaa;
	-webkit-box-shadow: 0 0 1px 1px rgba(130, 130, 130, .7);
	box-shadow: 0 0 1px 1px rgba(130, 130, 130, .7);
	z-index: 999;
}

.mui-slider-indicator .indicator-margin {
	margin-left: 0.05rem;
}


/*角标样式*/

.badge_red {
	position: absolute;
	right: 0;
	top: -2px;
	background-color: #ce0b24;
	border: 0.5px solid #fff;
	border-radius: 6px;
	font-size: 9px;
	color: #fff;
	line-height: 12px;
	padding: 0 4px;
	z-index: 100;
}


/*标签样式*/

.tag {
	display: inline-block;
	padding: 0rem 0.6rem;
	line-height: 1rem;
	font-size: 0.625rem;
	border-radius: 0.625rem;
	margin-right: 0.25rem;
}

.tag_green {
	color: #458E28 !important;
	background-color: #e8f7da;
	border: 1px solid #458F28;
}

.tag_orange {
	color: #F6A623 !important;
	background-color: #fdecd2;
	border: 1px solid #F5A623;
}

.tag_blue {
	color: #419BF9 !important;
	background-color: #f3f9fe;
	border: 1px solid #419BF9;
}

.tag_yellow {
	color: #AB8F4D !important;
	background-color: #f2ece0;
	border: 1px solid #c2a971;
}

/*关键词详情页样式*/

.tag_keyword {
	padding: 0rem 0.8rem;
    display: inline-block;
    line-height: 1.25rem;
    color: #4f87c8;
    background-color: #f5f5f5;
    font-size: 0.75rem;
    text-align: center;
    border-radius: 1.25rem;
    margin-top: 0.25rem;
    margin-right: 0.2rem;
}

.tag_keyword.tag_selected {
	color: #ff0000 !important;
}

/*相关推荐、评论区域样式*/

.div_related {
	padding: 1.6rem 1rem 0.4rem 1rem;
